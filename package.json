{
  "name": "angular-products",
  "version": "0.0.0",
  "main": "main.js",
  "build": {
    "appId": "org.iescamp.din.my-angular-app",
    "productName": "Mi Aplicaci√≥n Angular",
    "files": [
      "dist/angular-products/**/*",
      "main.js",
      "package.json"
    ]
  },
  "win": {
    "target": [
      "nsis"
    ]
  },
  "linux": {
    "target": [
      "AppImage"
    ]
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "dev": "npm-run-all --parallel electron start",
    "electron": "wait-on tcp:4200 && ng build --base-href ./ && electron .",
    "package": "ng build && electron-builder",
    "package-win": "ng build && electron-builder --win",
    "package-linux": "ng build && electron-builder --linux",
    "server": "json-server --watch server/db.json --port 3000"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/cdk": "^20.2.9",
    "@angular/common": "^20.3.0",
    "@angular/compiler": "^20.3.0",
    "@angular/core": "^20.3.0",
    "@angular/forms": "^20.3.0",
    "@angular/material": "^20.2.9",
    "@angular/platform-browser": "^20.3.0",
    "@angular/router": "^20.3.0",
    "bootstrap": "^5.3.8",
    "bootstrap-icons": "^1.13.1",
    "json-server": "^1.0.0-beta.3",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^20.3.6",
    "@angular/cli": "^20.3.6",
    "@angular/compiler-cli": "^20.3.0",
    "@types/jasmine": "~5.1.0",
    "electron": "^38.3.0",
    "electron-builder": "^26.0.12",
    "jasmine-core": "~5.9.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "npm-run-all": "^4.1.5",
    "typescript": "~5.9.2",
    "wait-on": "^9.0.1"
  },
  "description": "This project was generated using [Angular CLI](https://github.com/angular/angular-cli) version 20.3.6.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ZevsP/angular-products.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ZevsP/angular-products/issues"
  },
  "homepage": "https://github.com/ZevsP/angular-products#readme"
}
